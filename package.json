  "scripts": {
    "dev-server": "yarn build:email && yarn watch:email & encore dev-server --host localhost.drenso.dev --hot --https --cert /etc/apache2/ssl/drenso.dev/fullchain.pem --key /etc/apache2/ssl/drenso.dev/privkey.pem",
    "dev-server-bob": "yarn dev-server --port 8999",
    "dev-server-tobias": "yarn dev-server --port 8998",
    "dev": "yarn dev:email && yarn dev:encore",
    "dev:encore": "encore dev",
    "dev:email": "yarn build:email",
    "watch": "yarn watch:email & yarn watch:encore",
    "watch:encore": "encore dev --watch",
    "watch:email": "yarn build:email --watch",
    "build": "yarn build:email && yarn build:encore",
    "build:encore": "encore production",
    "build:email": "node-sass --importer=node_modules/node-sass-tilde-importer --output-style compressed ./assets/email/email.scss -o ./public/email"
  },
