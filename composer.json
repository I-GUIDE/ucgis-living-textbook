    "require": {
        "php": "^7.1.3",
        "ext-curl": "*",
        "ext-iconv": "*",
        "ext-dom": "*",
        "ext-libxml": "*",
        "bobv/latex-bundle": "^1.3.6",
        "components/elfinder": "^3.1.1",
        "drenso/symfony-oidc-bundle": "^v1.1",
        "easyrdf/easyrdf": "^0.9.1",
        "friendsofsymfony/ckeditor-bundle": "^2.0",
        "friendsofsymfony/jsrouting-bundle": "^2.1",
        "helios-ag/fm-elfinder-bundle": "^9.1.1",
        "jms/serializer-bundle": "^2.3",
        "kick-in/exception-handler-bundle": "^2.0",
        "ml/json-ld": "^1.1",
        "phpoffice/phpspreadsheet": "^1.5.1",
        "phpseclib/phpseclib": "^2.0",
        "ryakad/pandoc-php": "^1.0",
        "sensio/framework-extra-bundle": "^5.1",
        "sensiolabs/security-checker": "^5.0",
        "spatie/pdf-to-image": "^1.6",
        "stof/doctrine-extensions-bundle": "^1.3",
        "symfony/asset": "^4.2.1",
        "symfony/console": "^4.2.1",
        "symfony/dotenv": "^4.2.1",
        "symfony/expression-language": "^4.2.1",
        "symfony/filesystem": "^4.2.1",
        "symfony/flex": "^1.0",
        "symfony/form": "^4.2.1",
        "symfony/framework-bundle": "^4.2.1",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "^1.0",
        "symfony/proxy-manager-bridge": "^4.2.1",
        "symfony/security-bundle": "^4.2.1",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/translation": "^4.2.1",
        "symfony/twig-bundle": "^4.2.1",
        "symfony/validator": "^4.2.1",
        "symfony/webpack-encore-pack": "^1.0",
        "symfony/yaml": "^4.2.1",
        "twig/extensions": "^1.5"
    },
    "require-dev": {
        "drenso/phan-extensions": "^2.1",
        "symfony/phpunit-bridge": "^4.2.1",
        "symfony/profiler-pack": "^1.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "component-dir": "public/elfinder-assets",
        "cache-dir": "/var/cache/composer",
        "sort-packages": true,
        "gitlab-domains": [
            "gitlab.drenso.dev"
        ],
        "gitlab-token": {"gitlab.drenso.dev": "YMzbSnVVjy_jLkGGYbeF"}
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-apcu": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "ckeditor:install %PUBLIC_DIR%/ckeditor --exclude=samples --exclude=adapters --clear=drop -q": "symfony-cmd",
            "fos:js-routing:dump --target=assets/js/_fos_js_routes.js --callback=\"import Routing from 'fos-routing'; Routing.setData\"": "symfony-cmd",
            "security-checker security:check": "script"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01C1D83D8CDPBGH5RVQBHJ075A",
            "allow-contrib": false
        }
    }
}
