        "composer/package-versions-deprecated": "1.11.99.4",
        "doctrine/annotations": "1.13.2",
        "doctrine/collections": "1.6.8",
        "doctrine/common": "3.2.2",
        "doctrine/dbal": "3.3.1",
        "doctrine/doctrine-bundle": "2.5.5",
        "doctrine/doctrine-migrations-bundle": "3.2.2",
        "doctrine/event-manager": "1.1.1",
        "doctrine/migrations": "3.4.1",
        "doctrine/orm": "2.11.1",
        "doctrine/persistence": "2.3.0",
        "drenso/symfony-oidc-bundle": "1.12.0",
        "drenso/symfony-shared": "1.27.4",
        "easyrdf/easyrdf": "1.1.1",
        "friendsofsymfony/ckeditor-bundle": "2.4.0",
        "friendsofsymfony/jsrouting-bundle": "2.8.0",
        "gedmo/doctrine-extensions": "3.5.0",
        "helios-ag/fm-elfinder-bundle": "12.1.2",
        "jms/serializer": "3.17.1",
        "jms/serializer-bundle": "4.0.2",
        "kick-in/exception-handler-bundle": "3.0.1",
        "league/html-to-markdown": "5.0.2",
        "ml/json-ld": "1.2.0",
        "nelmio/api-doc-bundle": "4.8.2",
        "phpoffice/phpspreadsheet": "1.21.0",
        "rollerworks/password-strength-validator": "1.7.2",
        "ryakad/pandoc-php": "1.0.0",
        "sensio/framework-extra-bundle": "6.2.6",
        "sentry/sentry-symfony": "4.2.6",
        "spatie/pdf-to-image": "2.1.0",
        "stof/doctrine-extensions-bundle": "1.7.0",
        "symfony/asset": "4.4.37",
        "symfony/cache": "4.4.37",
        "symfony/config": "4.4.37",
        "symfony/console": "4.4.37",
        "symfony/dependency-injection": "4.4.37",
        "symfony/doctrine-bridge": "4.4.37",
        "symfony/dotenv": "4.4.37",
        "symfony/event-dispatcher": "4.4.37",
        "symfony/expression-language": "4.4.37",
        "symfony/filesystem": "4.4.37",
        "symfony/finder": "4.4.37",
        "symfony/flex": "2.1.4",
        "symfony/form": "4.4.37",
        "symfony/framework-bundle": "4.4.37",
        "symfony/http-foundation": "4.4.37",
        "symfony/http-kernel": "4.4.37",
        "symfony/lock": "4.4.37",
        "symfony/mailer": "4.4.37",
        "symfony/messenger": "4.4.37",
        "symfony/mime": "4.4.37",
        "symfony/monolog-bundle": "3.7.1",
        "symfony/options-resolver": "4.4.37",
        "symfony/process": "4.4.37",
        "symfony/property-access": "4.4.37",
        "symfony/proxy-manager-bridge": "4.4.37",
        "symfony/routing": "4.4.37",
        "symfony/security": "4.4.37",
        "symfony/security-bundle": "4.4.37",
        "symfony/string": "5.4.3",
        "symfony/translation": "4.4.37",
        "symfony/translation-contracts": "2.5.0",
        "symfony/twig-bridge": "4.4.37",
        "symfony/twig-bundle": "4.4.37",
        "symfony/validator": "4.4.37",
        "symfony/webpack-encore-bundle": "1.13.2",
        "symfony/yaml": "4.4.37",
        "twig/cssinliner-extra": "3.3.5",
        "twig/extra-bundle": "3.3.8",
        "twig/inky-extra": "3.3.5",
        "twig/intl-extra": "3.3.5",
        "twig/twig": "2.14.11",
        "zircote/swagger-php": "4.2.5"
    },
    "require-dev": {
        "drenso/phan-extensions": "3.5.1",
        "phpunit/phpunit": "9.5.13",
        "symfony/phpunit-bridge": "4.4.37",
        "symfony/stopwatch": "4.4.37",
        "symfony/web-profiler-bundle": "4.4.37"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "component-dir": "public/elfinder-assets",
        "cache-dir": "/var/cache/composer",
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-apcu": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "ckeditor:install %PUBLIC_DIR%/ckeditor --exclude=samples --exclude=adapters --clear=drop -q": "symfony-cmd",
            "fos:js-routing:dump --target=assets/js/_fos_js_routes.js --callback=\"import Routing from 'fos-routing'; Routing.setData\"": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.4.*"
        },
        "unused": [
            "league/html-to-markdown",
            "symfony/messenger",
            "symfony/proxy-manager-bridge",
            "twig/cssinliner-extra",
            "twig/extensions",
            "twig/inky-extra",
            "twig/intl-extra"
        ]
    }
}
