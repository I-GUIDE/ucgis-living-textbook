    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "component-dir": "public/elfinder-assets",
        "cache-dir": "/var/cache/composer",
        "sort-packages": true,
        "gitlab-domains": [
            "gitlab.drenso.dev"
        ],
        "gitlab-token": {"gitlab.drenso.dev": "YMzbSnVVjy_jLkGGYbeF"}
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-apcu": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "ckeditor:install %PUBLIC_DIR%/ckeditor --exclude=samples --exclude=adapters --clear=drop -q": "symfony-cmd",
            "fos:js-routing:dump --target=assets/js/_fos_js_routes.js --callback=\"import Routing from 'fos-routing'; Routing.setData\"": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01C1D83D8CDPBGH5RVQBHJ075A",
            "allow-contrib": false,
            "require": "4.4.*"
        },
        "unused": [
            "league/html-to-markdown",
            "symfony/messenger",
            "symfony/proxy-manager-bridge",
            "twig/cssinliner-extra",
            "twig/extensions",
            "twig/inky-extra"
        ]
    }
